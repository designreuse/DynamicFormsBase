<?xml version="1.0" encoding="utf-8"?>
<df:dynaform xmlns:df="http://www.dynamicforms.mx/dynaform-1.0">
	<df:row>
		<df:label text="Short Name" for="ShortName" />
		<df:label text="Street" />
	</df:row>
	<df:row>
		<df:input id="ShortName" required="true" />
		<df:input id="Street" colspan="2" />
	</df:row>
	<df:row>
		<df:label text="Long Name" for="LongName" />
		<df:label text="Zip Code, City" />
	</df:row>
	<df:row>
		<df:input id="LongName" required="true" />
		<df:input id="ZipCity" colspan="2" />
	</df:row>
	<df:row>
		<df:separator text="MakeChoice" colspan="3" />
	</df:row>
	<df:row>
		<df:label text="PrimeFaces fan?" />
		<df:label text="Married?" />
		<df:label text="Children" />
	</df:row>
	<df:row>
		<df:radiochoice id="fan">
			<df:selectItem>
				<df:label>Sure</df:label>
				<df:value>Sure</df:value>
			</df:selectItem>
			<df:selectItem>
				<df:label>Don't know</df:label>
				<df:value>Don't know</df:value>
			</df:selectItem>
		</df:radiochoice>
		<df:booleanchoice id="Married" />
		<df:radiochoice id="Children">
			<df:selectItem>
				<df:label>No</df:label>
				<df:value>No</df:value>
			</df:selectItem>
			<df:selectItem>
				<df:label>1</df:label>
				<df:value>1</df:value>
			</df:selectItem>
			<df:selectItem>
				<df:label>2</df:label>
				<df:value>2</df:value>
			</df:selectItem>
			<df:selectItem>
				<df:label>More</df:label>
				<df:value>More</df:value>
			</df:selectItem>
		</df:radiochoice>
	</df:row>
</df:dynaform>
